    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/Socks5] Test IPv4",
            "program": "${workspaceFolder}/src/examples/test-ipv4.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "lrt_socks5_host": "127.0.0.1",
                "lrt_socks5_port": "1080",
                "lrt_socks5_username": "admin",
                "lrt_socks5_password": "qwer1234"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/Socks5] Test Domain",
            "program": "${workspaceFolder}/src/examples/test-domain.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "lrt_socks5_host": "127.0.0.1",
                "lrt_socks5_port": "1080",
                "lrt_socks5_username": "admin",
                "lrt_socks5_password": "qwer1234"
            }
        }
    ]
}